{
    "name": "samba-localization-highlighter",
    "displayName": "Samba Localization Highlighter",
    "description": "Provides syntax highlighting for Discord-Samba's Localization file format",
	"version": "1.0.3",
	"repository": {
		"url": "https://github.com/discord-samba/localization-highlighter"
	},
	"publisher": "discord-samba",
    "engines": {
        "vscode": "^1.42.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "samba-localization",
            "aliases": ["Samba Localization", "samba-localization"],
            "extensions": [".lang"]
        }],
        "grammars": [{
            "language": "samba-localization",
            "scopeName": "source.lang",
			"path": "./syntaxes/samba-localization.tmLanguage.json",
			"embeddedLanguages": {
				"meta.embedded.block.javascript": "javascript"
			}
        }]
    }
}